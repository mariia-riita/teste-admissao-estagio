def fizz_buzz():
 #Loop de 1 a 100
 for i in range (1, 101):
  #Verifico se o número é múltiplo de 15 (múltiplo de 3 e 5)
  if i % 15 == 0:
     print("FizzBuzz")
  #Verifico se o número é múltiplo de 5
  elif i % 5 ==0:
     print("Fizz")
  #Verifico se o número é múltiplo de 3
  elif i % 3 == 0:
     print("Buzz")
  "Se não for múltiplo de nenhum dos casos anteriores, imprimo o número
  else:
      print(i)

if _ _ name _ _ == "_ _main_ _":
     fizz_buzz()
